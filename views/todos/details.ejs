<div class="todo-container">
    <div id="<%-todo._id%>" class="container"><%- todo.text %></div>
    <div class="mt-4">
        <form action="/dismiss/<%- todo._id%>" method="POST">
            <a href="/" type="button" class="btn btn-danger">Cancel</a>
            <button type="submit" class="btn btn-danger">Dismiss</button>
        </form>
    </div>
</div>